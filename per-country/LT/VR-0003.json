              },
              {
                "if": [
                  {
                    ">": [
                      {
                        "var": "payload.v.0.dn"
                      },
                      1
                    ]
                  },
                  true,
                  false
                ]
              },
              {
                "if": [
                  {
                    "and": [
                      {
                        "===": [
                          {
                            "var": "payload.v.0.dn"
                          },
