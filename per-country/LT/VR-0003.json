                          {
                            "var": "payload.v.0.dn"
                          },
                          2
                        ]
                      },
                      true,
                      false
                    ]
                  },
                  false
                ]
              }
            ]
          }
        ]
      },
      true
    ]
  },
  "expr-as-text": "if /payload.v.0 then if (/external.validationClock +0 days) not-before (/payload.v.0.dt +14 days) then true else if (/payload.v.0.mp) === (\"EU/1/20/1525\") then if (/payload.v.0.dn) > (1) then true else false else if ((/payload.v.0.dn) === (2)) and ((/payload.v.0.sd) === (1)) then true else if (/payload.v.0.dn) > (2) then true else false else true"
