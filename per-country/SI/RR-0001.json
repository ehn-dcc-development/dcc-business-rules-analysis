{
  "Identifier": "RR-SI-0001",
  "Type": "Acceptance",
  "Country": "SI",
  "Version": "1.0.0",
  "SchemaVersion": "1.0.0",
  "Engine": "CERTLOGIC",
  "EngineVersion": "0.7.5",
  "CertificateType": "Recovery",
  "Description": [
    {
      "lang": "en",
      "desc": "The Verification Datetime must be between \"Certificate Valid From\" and \"Certificate Valid Until\"."
    },
    {
      "lang": "si",
      "desc": "Veljavnost potrdila mora biti med \"Certificate Valid From\" and \"Certificate Valid Until\"."
    }
  ],
  "ValidFrom": "2021-07-30T00:00:00Z",
  "ValidTo": "2030-06-01T00:00:00Z",
  "AffectedFields": [
    "r.0",
    "r.0.df",
    "r.0.du"
  ],
  "Logic": {
    "if": [
      {
        "var": "payload.r.0"
      },
      {
        "not-after": [
          {
            "plusTime": [
              {
                "var": "payload.r.0.df"
              },
              0,
              "day"
            ]
          },
          {
            "plusTime": [
              {
                "var": "external.validationClock"
              },
              0,
              "day"
            ]
          },
          {
            "plusTime": [
              {
                "var": "payload.r.0.du"
              },
              0,
              "day"
            ]
          }
        ]
      },
      true
    ]
  },
  "expr-as-text": "if /payload.r.0 then (/payload.r.0.df +0 days) not-after (/external.validationClock +0 days) not-after (/payload.r.0.du +0 days) else true"
}