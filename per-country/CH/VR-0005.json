      "v.0",
      "v.0.dn",
      "v.0.dt"
    ],
    "Logic": {
      "if": [
        {
          "var": "payload.v.0"
        },
        {
          "if": [
            {
              "and": [
                {
                  "in": [
                    {
                      "var": "payload.v.0.mp"
                    },
                    [
                      "EU/1/20/1525"
