    "CertificateType": "Vaccination",
    "Description": [
      {
        "lang": "en",
        "desc": "Verification Datetime must be less than 365 days after the date of booster dose."
      },
      {
        "lang": "ee",
        "desc": "Tõhustusdoosist ei tohi olla möödas rohkem kui 365 päeva."
      }
    ],
    "ValidFrom": "2022-02-01T00:00:00Z",
    "ValidTo": "2023-02-01T00:00:00Z",
    "AffectedFields": [
      "v.0",
      "v.0.dt",
      "v.0.sd",
      "v.0.dn"
    ],
    "Logic": {
      "if": [
        {
          "var": "payload.v.0"
        },
        {
          "if": [
            {
              ">": [
                {
                  "var": "payload.v.0.dn"
                },
                {
                  "var": "payload.v.0.sd"
                }
              ]
            },
            {
              "not-after": [
                {
                  "plusTime": [
                    {
                      "var": "external.validationClock"
                    },
                    0,
                    "day"
                  ]
                },
                {
                  "plusTime": [
                    {
                      "var": "payload.v.0.dt"
                    },
                    365,
                    "day"
                  ]
                }
              ]
            },
            true
          ]
        },
        true
      ]
    },
    "expr-as-text": "if /payload.v.0 then if (/payload.v.0.dn) > (/payload.v.0.sd) then (/external.validationClock +0 days) not-after (/payload.v.0.dt +365 days) else true else true"
  }
]