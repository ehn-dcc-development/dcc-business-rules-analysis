            },
            {
              "plusTime": [
                {
                  "var": "payload.r.0.fr"
                },
                10,
                "day"
              ]
            }
          ]
        },
        true
      ]
    },
    "expr-as-text": "if (/payload.r.0) then (((/external.validationClock) +0 days) not-before ((/payload.r.0.fr) +10 days)) else (true)"
  }
